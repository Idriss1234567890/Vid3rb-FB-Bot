{
  "version": 2,
  "builds": [
    {
      "src": "api/webhook.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/webhook",
      "dest": "/api/webhook.js"
    }
  ],
  "functions": {
    "api/webhook.js": {
      "maxDuration": 300,
      "memory": 3008
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}
